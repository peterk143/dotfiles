#--shortcuts--#
alias l='ls -l'
alias f='clear'
alias la='ls -lA'
alias cp='cp -v'
alias mv='mv -v'
alias sc='screen'
alias lisa='ls -lisA'
alias asdf='cd && clear'
alias lds='ldapsearch -xLLL'
alias utz=logout''


#--programs--#
alias nano='nano -S'
alias emacs='emacs -nw'
alias chat='weechat-curses'

#--maintenance--#
alias fix='sudo yum -y update'
alias cleanem='sudo rm -rf /var/cache/yum/* && yum clean all'
